{
  "expo": {
    "name": "copyflix",
    "slug": "copyflix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/netflixlogo@1.png",
    "splash": {
      "image": "./src/assets/images/netflixlogo@1.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/netflixlogo@1.png",
        "backgroundColor": "#000"
      }
    },
    "web": {
      "favicon": "./src/assets/images/netflixlogo@1.png"
    }
  }
}
